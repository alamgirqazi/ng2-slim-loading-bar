{
  "name": "ng2-al-slim-bar",
  "description": "Angular2 component shows slim loading bar at the top of the page",
  "version": "0.0.1",
  "scripts": {
    "test": "karma start",
    "test-watch": "tsc && karma start --no-single-run --auto-watch",
    "commit": "npm run prepublish && npm test && git-cz",
    "prepublish": "ngc && npm run build",
    "build": "webpack && cp style.css bundles/style.css",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "analyze": "ANALYZE=true webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alamgirqazi/ng2-slim-loading-bar"
  },
  "keywords": [
    "angular",
    "angular2",
    "slim",
    "loading",
    "bar",
    "progress"
  ],
  "author": "Alamgir Qazi",
  "license": "MIT",
  "main": "bundles/index.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "homepage": "https://github.com/alamgirqazi/ng2-slim-loading-bar",
  "peerDependencies": {},
  "devDependencies": {
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "3.5.0",
    "@types/node": "12.12.14",
    "awesome-typescript-loader": "5.2.1",
    "codelyzer": "5.2.0",
    "commitizen": "4.0.3",
    "core-js": "^3.4.2",
    "cz-conventional-changelog": "3.0.2",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "3.5.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.1",
    "karma-firefox-launcher": "1.2.0",
    "karma-jasmine": "2.0.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-remap-coverage": "~0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "loader-utils": "~1.2.3",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^6.5.3",
    "semantic-release": "15.13.31",
    "source-map-loader": "0.2.4",
    "ts-helpers": "1.1.2",
    "tslint": "~5.20.1",
    "tslint-loader": "3.5.4",
    "typescript": "3.5.3",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "zone.js": "^0.10.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "rxjs-compat": "^6.5.3"
  }
}
